{
  "Serilog": {
    "Using": [
      "Serilog.Enrichers.Sensitive"
    ],
    "Enrich": [
      {
        "Name": "WithSensitiveDataMasking",
        "Args": {
          "options": {
            "MaskValue": "**SECRET**",
            "MaskProperties": [
              { "Name": "secret"},
              { "Name": "propwithoptions",
                "Options": {
                  "ShowFirst": 3
                }
              }
            ]
          }
        }
      }
    ]
  }
}